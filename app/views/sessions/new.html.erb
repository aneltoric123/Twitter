<h1>Login</h1>

<%= form_with(url: login_path) do |form| %>
  <%= form.label :email %>
  <%= form.email_field :email %>

  <%= form.label :password %>
  <%= form.password_field :password %>

  <%= form.submit 'Login' %>
<% end %>
<%= link_to 'Go back to Homepage', root_path, class: 'btn btn-primary' %>

